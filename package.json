{
  "name": "vincsslocker",
  "version": "1.0.13",
  "private": true,
  "author": "Locker",
  "description": "Locker PM Desktop self-hosted",
  "dependencies": {
    "@lockerpm/design": "^1.0.17",
    "apexcharts": "^3.44.2",
    "axios": "^0.27.2",
    "big-integer": "^1.6.51",
    "browser-hrtime": "^1.1.8",
    "credit-card-type": "^10.0.0",
    "electron-is-dev": "^2.0.0",
    "electron-store": "^8.1.0",
    "extract-domain": "^4.1.5",
    "i18next": "^22.5.0",
    "locker-desktop-service": "^0.2.1",
    "lodash": "^4.17.21",
    "lunr": "^2.3.9",
    "moment": "^2.29.4",
    "node-forge": "^1.3.1",
    "papaparse": "^5.4.1",
    "react": "^18.2.0",
    "react-apexcharts": "^1.4.0",
    "react-dom": "^18.2.0",
    "react-i18next": "^12.3.1",
    "react-intl": "^6.0.5",
    "react-qr-code": "^2.0.8",
    "react-redux": "^8.0.2",
    "react-router-dom": "^6.3.0",
    "react-scripts": "^5.0.1",
    "react-slick": "^0.29.0",
    "react-use-websocket": "^4.4.0",
    "redux": "^4.2.0",
    "redux-thunk": "^2.4.1",
    "slick-carousel": "^1.8.1",
    "sweetalert2": "^10.16.7",
    "tldjs": "^2.3.1",
    "totp-generator": "^0.0.14",
    "url": "^0.11.1",
    "web-vitals": "^2.1.4",
    "zxcvbn": "^4.4.2"
  },
  "main": "public/electron.js",
  "homepage": "./",
  "productName": "VinCSS Password Manager",
  "scripts": {
    "start": "concurrently \"set BROWSER=none && set HTTPS=true&&craco start\" \"wait-on https://demo.locker.io:3000 && electron .\"",
    "start-web": "set BROWSER=none && set HTTPS=true && craco start",
    "start-desktop": "electron .",
    "build": "craco build",
    "eject": "react-scripts eject",
    "release": "CI=false craco build && electron-builder --publish=always",
    "release:mac": "yarn gen-cert && craco build && electron-builder -c ./build-config/electron-builder.macos.json",
    "release:win-64": "setx CI false && craco build && electron-builder -c ./build-config/electron-builder.windows.json",
    "sub:init": "git submodule update --init --recursive",
    "sub:pull": "git submodule foreach git pull origin vincss",
    "sub:update": "git submodule update --remote",
    "gen-cert": "sh macos-config/cert_gen.sh"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@craco/craco": "^6.4.5",
    "@types/node-forge": "^1.3.3",
    "@types/papaparse": "^5.3.7",
    "@types/tldjs": "^2.3.1",
    "@types/zxcvbn": "^4.4.1",
    "babel-plugin-import": "^1.13.8",
    "concurrently": "^8.2.2",
    "craco-less": "^2.0.0",
    "dotenv": "^16.3.1",
    "electron": "^27.1.0",
    "electron-builder": "^24.6.4",
    "eslint": "^8.50.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-import-resolver-typescript": "^3.6.1",
    "eslint-plugin-import": "^2.28.1",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "node-sass": "^8.0.0",
    "prettier": "^3.0.3",
    "prettier-eslint": "^15.0.1",
    "tailwindcss": "^3.3.2",
    "wait-on": "^7.2.0"
  }
}
