  "main": "public/electron.js",
  "homepage": "./",
  "productName": "Locker Password Manager",
  "build": {
    "appId": "com.lockerpm_app",
    "extraResources": [
      ".env-cmdrc"
    ],
    "files": [
      "build/**/*",
      "node_modules/**/*",
      "package.json",
      "cert/**/*"
    ],
    "win": {
      "target": "nsis"
    }
  },
  "scripts": {
    "start": "concurrently \"SET BROWSER=none && set HTTPS=true&&env-cmd -e development craco start\" \"wait-on https://demo.locker.io:3000 && env-cmd -e development electron .\"",
    "build:dev": "env-cmd -e development craco build",
    "build:stg": "env-cmd -e staging craco build",
    "build": "craco build",
    "test": "craco test",
    "eject": "react-scripts eject",
    "sub:init": "git submodule update --init --recursive",
    "sub:pull": "git submodule foreach git pull origin dev",
    "sub:update": "git submodule update --remote",
    "release": "env-cmd -e development craco build && env-cmd -e development electron-builder --publish=always"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "electron": "^27.1.0",
    "electron-builder": "^24.6.4"
  }
}
