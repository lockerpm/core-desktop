{
  "name": "lockerpm",
  "version": "1.0.0",
  "private": true,
  "dependencies": {
    "@lockerpm/design": "^1.0.17",
    "apexcharts": "^3.44.2",
    "axios": "^0.27.2",
    "big-integer": "^1.6.51",
    "browser-hrtime": "^1.1.8",
    "craco-less": "^2.0.0",
    "credit-card-type": "^10.0.0",
    "dotenv": "^16.3.1",
    "electron-is-dev": "^2.0.0",
    "electron-store": "^8.1.0",
    "extract-domain": "^4.1.5",
    "i18next": "^22.5.0",
    "locker-desktop-service": "^0.1.3",
    "lodash": "^4.17.21",
    "lunr": "^2.3.9",
    "moment": "^2.29.4",
    "node-forge": "^1.3.1",
    "papaparse": "^5.4.1",
    "react": "^18.2.0",
    "react-apexcharts": "^1.4.0",
    "react-dom": "^18.2.0",
    "react-i18next": "^12.3.1",
    "react-intl": "^6.0.5",
    "react-qr-code": "^2.0.8",
    "react-redux": "^8.0.2",
    "react-router-dom": "^6.3.0",
    "react-scripts": "^5.0.1",
    "react-slick": "^0.29.0",
    "react-use-websocket": "^4.4.0",
    "redux": "^4.2.0",
    "redux-thunk": "^2.4.1",
    "slick-carousel": "^1.8.1",
    "sweetalert2": "^10.16.7",
    "tailwindcss": "^3.3.2",
    "tldjs": "^2.3.1",
    "totp-generator": "^0.0.14",
    "url": "^0.11.1",
    "web-vitals": "^2.1.4",
    "zxcvbn": "^4.4.2"
  },
  "main": "public/electron.js",
  "homepage": "./",
  "productName": "Locker Password Manager",
  "build": {
    "appId": "com.lockerpm_app",
    "files": [
      "build/**/*",
      "node_modules/**/*",
      "package.json",
      "installer/**/*",
      "service/**/*"
    ],
    "win": {
      "target": "nsis"
    },
    "nsis": {
      "oneClick": false,
      "perMachine": true,
      "allowElevation": false,
      "allowToChangeInstallationDirectory": true,
      "deleteAppDataOnUninstall": true,
      "runAfterFinish": false,
      "include": "service/installer.nsh",
      "uninstallDisplayName": "locker-desktop-uninstaller"
    },
    "extraFiles": [
      {
        "from": "service/locker_service.exe",
        "to": "locker_service.exe"
      },
      {
        "from": "service/cbor.dll",
        "to": "cbor.dll"
      },
      {
        "from": "service/crypto-50.dll",
        "to": "crypto-50.dll"
      },
      {
        "from": "service/fido2.dll",
        "to": "fido2.dll"
      },
      {
        "from": "service/zlib1.dll",
        "to": "zlib1.dll"
      },
      {
        "from": "service/cert-gen-and-service-install.ps1",
        "to": "cert-gen-and-service-install.ps1"
      },
      {
        "from": "service/cert-gen-and-service-uninstall.ps1",
        "to": "cert-gen-and-service-uninstall.ps1"
      }
    ]
  },
  "scripts": {
    "start": "concurrently \"SET BROWSER=none && set HTTPS=true&&craco start\" \"wait-on https://demo.locker.io:3000 && electron .\"",
    "build": "craco build",
    "eject": "react-scripts eject",
    "release": "craco build && electron-builder --publish=always",
    "release:win-64": "craco build && electron-builder build --win --x64",
    "sub:init": "git submodule update --init --recursive",
    "sub:pull": "git submodule foreach git pull origin vincss",
    "sub:update": "git submodule update --remote"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@craco/craco": "^6.4.5",
    "@types/node-forge": "^1.3.3",
    "@types/papaparse": "^5.3.7",
    "@types/tldjs": "^2.3.1",
    "@types/zxcvbn": "^4.4.1",
    "babel-plugin-import": "^1.13.8",
    "concurrently": "^8.2.2",
    "electron": "^27.1.0",
    "electron-builder": "^24.6.4",
    "eslint": "^8.50.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-import-resolver-typescript": "^3.6.1",
    "eslint-plugin-import": "^2.28.1",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "node-sass": "^8.0.0",
    "prettier": "^3.0.3",
    "prettier-eslint": "^15.0.1",
    "typescript": "^5.1.6",
    "wait-on": "^7.2.0"
  }
}